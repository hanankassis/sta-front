import makeApiRequest from "./api";

const admin = {
  list: async (data) =>
    makeApiRequest({ verb: "get", url: "/admin/providers", data }),
  toggleState: async (id) =>
    makeApiRequest({ verb: "patch", url: `/admin/providers/${id}` }),
  totals: async () =>
    makeApiRequest({ verb: "get", url: "/admin/totals" }),
};
export default admin;
